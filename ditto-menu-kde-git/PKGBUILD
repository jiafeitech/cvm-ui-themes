pkgname=ditto-menu-kde-git
pkgver=1
pkgrel=1
pkgdesc="Menu launcher for plasma KDE"
arch=('any')
makedepends=('git')
source=("git+https://github.com/adhec/dittoMenuKDE.git")
sha256sums=('SKIP')

package() {
   mkdir -p $pkgdir/usr/share/plasma/plasmoids
   cp -r $srcdir/dittoMenuKDE/package $pkgdir/usr/share/plasma/plasmoids/org.kde.plasma.dittomenu
} 
